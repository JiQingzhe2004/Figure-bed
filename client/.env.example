# API配置
# 本地API地址(开发机器访问时使用)
REACT_APP_LOCAL_API_URL=http://localhost:5000
# 远程API地址(通过网络或移动设备访问时使用)
REACT_APP_REMOTE_API_URL=http://192.168.48.209:5000

# 如果设置为true，将尝试自动判断使用哪个API地址
REACT_APP_AUTO_SELECT_API=true

# 超时和其他配置
REACT_APP_API_TIMEOUT=15000
REACT_APP_API_WITH_CREDENTIALS=false
REACT_APP_API_CONTENT_TYPE=application/json
REACT_APP_API_ACCEPT=application/json

# 启用跨域调试
REACT_APP_ENABLE_CORS_DEBUG=true

# 认证相关配置
REACT_APP_AUTH_STORAGE_KEY=token
